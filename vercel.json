{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/translate",
      "dest": "/api/translate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/l.html"
    }
  ]
} 